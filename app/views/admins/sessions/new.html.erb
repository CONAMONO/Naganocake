<div class = "container">
  <div class = "row">
    <div class = "col-xs-4"></div>
    <div class = "col-xs-4">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <p>メールアドレス</p><%= f.email_field :email, autofocus: true,autocomplete: "メールアドレス" %>

        <p>パスワード</p><%= f.password_field :password, autocomplete: "パスワード" %>

      <% if devise_mapping.rememberable? %>
        <div class="field">
          <%= f.check_box :remember_me %>
          <%= f.label :remember_me %>
        </div>
      <% end %>

      <div class="actions">
        <button type="submit" class="btn btn-primary btn-block">ログイン</button>
      </div>
    <% end %>

    <%= render "admins/shared/links" %>
  </div>
  <div class = "col-xs-4"></div>
</div>
